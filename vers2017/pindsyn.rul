REMOVE (pl @<Q) (NEGATE *-1 NimKv OR KvAdv BARRIER CLB OR Oeldis);
REMOVE (@<Q) (-1 (S))(-2 (ord)); # 1989. aasta valimisi


REMOVE (@<Q) (NEGATE *-1 ArvKv OR NimKv BARRIER CLB)
             (NOT -1 KvAdv)(NOT -2 KvAdv)(NOT -3 KvAdv);
REMOVE (sg @<Q) (NEGATE *-1 ArvKv OR KvAdv BARRIER CLB)
             (*-1 NimKv + Pl);
REMOVE (@<Q) (*-1 ArvKv BARRIER CLB)(*1C (@<Q) BARRIER NimKv OR KvAdv OR ArvKv);
REMOVE (@<Q) (*-1 ArvKv BARRIER CLB)(1C (@<Q));
REMOVE (part @<Q) (-1 JaVoi)(-2 (part))(NOT -2 (@<Q));
REMOVE (@<Q) (-1 Verb);
REMOVE (@<Q) (*-1 ArvKv BARRIER CLB OR NimKv OR KvAdv LINK 1 Adjühik);
REMOVE (part @<Q) (*-1 NimKv BARRIER CLB OR ArvKv LINK NOT 0 (nom) OR (gen) OR (part));
SELECT (@<Q) (NOT 0 (gen))(NOT -1 (Com))
	(*-1 NimKv OR ArvKv OR KvAdv BARRIER NotEestäiend OR CLB LINK 0 (nom) LINK NOT 1 (Com)); #TP +-1Com
SELECT (@<Q) (NOT -1 (Com))(NEGATE *1 (@<Q) BARRIER NotEestäiend)
	(*-1 NimKv OR ArvKv OR KvAdv BARRIER NotEestäiend OR CLB LINK 0 (nom) LINK NOT 1 (Com)); #TP eelmine löödud kaheks: kas järgm pole <Q või ise pole gen
REMOVE (digit @<Q) (-1 (Y));
#Kui sõna võib olla kvantori laiend ja kvantor ise võib olla alus, siis
#kustuta subjekti märgend
#Laual oli kuhi musti toidunõusid
REMOVE (@SUBJ)(0 (@<Q))(NOT 0 (nom))
        (*-1 NimKv + Subjekt OR ArvKv + Subjekt OR KvAdv + Subjekt BARRIER CLB LINK NOT 1 (Com));


#VIII KAASSONAFRAASI KUULUVAD POHJAD

# 1) Leidub ühene kaassona
# a) tagasõna
# tagasona korral tuleb kontrollida, ega sona ei kuulu kvanorifraasi
SELECT (@P>) (1 (post))(NOT 0 (@<Q));
# kvantorid kaassõnafraasis
SELECT (@P>) (0 NimKv OR ArvKv OR KvAdv)(1C (@<Q))(2 (post));
SELECT (@P>) (0 NimKv OR ArvKv OR KvAdv)(1 Eestäiend)(2C (@<Q))(3 (post));
# kahe _vihkamise_ minuti jooksul
REMOVE (@P>) (-1 NimKv OR ArvKv OR KvAdv)(0 Eestäiend)(1C (@<Q))(2 (post));

# b) eessõna
# sona peab ka eessonaga ühilduma
SELECT (term @<P) (*-1 (pre <term>) BARRIER CLBC);
SELECT (all @<P) (*-1 (pre <all>) BARRIER CLBC);
SELECT (el @<P) (*-1 (pre <el>) BARRIER CLBC)(NEGATE *1 (el) BARRIER CLBC);
SELECT (abes @<P) (*-1 (pre <abes>) BARRIER CLBC);
SELECT (kom @<P) (*-1 (pre <kom>) BARRIER CLBC);

SELECT (part @<P) (*-1C (pre <part>) BARRIER (part @<P) OR CLBC)
             (*1 NPpiir BARRIER (part @<P) OR CLBC);
SELECT (part @<P) (*-1C (pre <part>) BARRIER (part @<P) OR CLBC)
             (NEGATE *1 (part @<P) BARRIER CLBC);
SELECT (part @<P) (*-1C (pre <part>) BARRIER (part @<P) OR CLBC)
             (1 ("ja") OR ("ning"));

SELECT (gen @<P) (*-1C (pre <gen>) BARRIER (gen @<P) OR CLBC)
             (*1 NPpiir BARRIER (gen @<P) OR CLBC);
SELECT (gen @<P) (*-1C (pre <gen>) BARRIER (gen @<P) OR CLBC)
             (NEGATE *1 (gen @<P) BARRIER CLBC);
SELECT (gen @<P) (*-1C (pre <gen>) BARRIER (gen @<P) OR CLBC)
             (1 ("ja") OR ("ning"));

#2) Lauses ei leidu teisi kaassonafraasi kuuluvaid pohju
SELECT (@P>) (NEGATE *-1 (@P>) BARRIER CLBC OR Oeldis)
             (*1C (post) BARRIER (@P>) OR CLBC);
SELECT (@<P) (NEGATE *1 (@<P) BARRIER CLBC OR Oeldis)
             (*-1C (pre) BARRIER (@<P) OR CLBC);

#3) Juba leidus postpositsiooni pohi
REMOVE (@P>) (1C (@P>));
REMOVE (@P>) (*1C (post) BARRIER CLB OR Sid|Kvm LINK -1C (@P>));
REMOVE (@P>) (*1C (post) BARRIER CLB LINK -1C (@P>)) (NOT 1 Sid|Kvm);
REMOVE (@P>) (*1C (post) BARRIER CLB LINK *-1 (@P>) LINK NOT 0 Järeltäiend) 
             (NOT 1 Sid|Kvm);
# juba leidus prepositsiooni pohi
REMOVE (@<P) (*-1 (pre) BARRIER CLB OR Sid|Kvm LINK *1C (@<P));
REMOVE (@<P) (*-1 (pre) BARRIER CLB)(*1C (@<P) BARRIER CLB OR Sid|Kvm);

# 4) Antud sona ja kaassona vahele jääb verb
REMOVE (@P>) (NEGATE *1 (post) BARRIER CLB);
REMOVE (@P>) (*1 Oeldis BARRIER (post) OR CLB);
REMOVE (@<P) (NEGATE *-1 (pre) BARRIER CLB);
REMOVE (@<P) (*-1 Oeldis BARRIER (pre) OR CLB);

#7) Koordinatsioon
# tuleb vältida: valdade ja väikelinnade esindajate seas

SELECT (gen @P>) (1 JaVoiK)(*2C (gen @P>) BARRIER Verb OR CLBC)
             (NOT 2 Eestäiend);
SELECT (gen @P>) (0 (gen))(1 JaVoiK)(*2C (gen @P>) BARRIER Verb OR CLB)
             (2 (@AN>) OR (@DN>) OR (@KN>) OR (@INFN>));
SELECT (gen @P>) (1 JaVoiK)(*2C (gen @P>) BARRIER Verb OR CLB)(NOT 2 (gen));

SELECT (part @P>) (1 JaVoiK)(*2C (part @P>) BARRIER Verb OR CLB);
SELECT (el @P>) (1 JaVoiK)(*2C (el @P>) BARRIER Verb OR CLB);

SELECT (@P>) (1 (Opr))(*2 (CPr) LINK 1 (post));

SELECT (gen @<P) (NOT 0 Eestäiend)
             (*-2C (gen @<P) BARRIER Verb OR CLB LINK 1 JaVoi);
SELECT (part @<P) (NOT 0 Eestäiend)
             (*-2C (part @<P) BARRIER Verb OR CLB LINK 1 JaVoi);

SELECT (gen @<P) (-1 JaVoi)(-2 (gen))(-2C (@<P))(NOT 1 (S));
SELECT (sg part @<P) (-1 (sg part))(-2 JaVoi)(-3 (sg part))(-3C (@<P));

#8) Kaks omastavas käändes nimisona eessonafraasi
# Päike paistis läbi köögiukse klaasi ja sitskardina
# Mis korra kuus üle mu lagede suundusid
# Kui verb on intransitiivne, on viimane sona p_hi.
REMOVE (@<P) (-1 (pre <gen>))(0 (gen) ^ Arv)(1 (gen))(NOT 0 NimKv);
# Verbi intransitiivsust esialgu ei kontrolli.
SELECT (@<P) (-1 (pre <gen>))(0 (gen) + Arv)(NOT 1 Arv);
# üle ühe mu elu votmestseeni


# II SUBJEKTIGRAMMATIKA

# 1) Verbi vorm välistab subjekti olemasolu 
# subjekti ei saa olla, kui selles osalauses on öeldis impersonaalis
REMOVE (@SUBJ) (*-1C Impl BARRIER CLBC OR Sid|Kvm LINK 0C Oeldis);
REMOVE (@SUBJ) (*1C Impl BARRIER CLBC OR Sid|Kvm LINK 0C Oeldis);

# 2) Sõna ei ühildu öeldisega
# Erand: eks _me_ tee neid tükke vahel nüüdki
SELECT (P pers nom @SUBJ) (*1 (imper ps2 sg ) BARRIER CLB)
                (*-1 ("eks") BARRIER CLB);

# kui osalause öeldiseks on verb 1. või 2. pöördes, siis subjektiks
# saavad olla ainult isikulised asesõnad (vastavalt verbi vormile
# "mina", "sina", "meie", "teie"), kõik muu subjektiks ei ole

REMOVE (@SUBJ) (NOT 0 ("mina") OR ("ma"))(*-1C (V ps1 sg) BARRIER (Z) OR CLBC);
REMOVE (@SUBJ) (NOT 0 ("mina") OR ("ma"))(*1C (V ps1 sg) BARRIER (Z) OR CLBC);

REMOVE (@SUBJ) (NOT 0 ("sina") OR ("sa"))(*-1C (V ps2 sg) BARRIER (Z) OR CLBC);
REMOVE (@SUBJ) (NOT 0 ("sina") OR ("sa"))(*1C (V ps2 sg) BARRIER (Z) OR CLBC);
# Viga tekkis: eks nad varsti tee jälle süsti

REMOVE (@SUBJ) (NOT 0 (pers ps1) OR ("kõik"))
               (*1C (V ps1 pl) BARRIER Sid|Kvm OR CLBC);
REMOVE (@SUBJ) (NOT 0 (pers ps1) OR ("kõik"))
               (*-1C (V ps1 pl) BARRIER Sid|Kvm OR CLBC);
REMOVE (@SUBJ) (NOT 0 (pers ps2))(*1C (V ps2 pl) BARRIER Sid|Kvm OR CLBC);
REMOVE (@SUBJ) (NOT 0 (pers ps2))(*-1C (V ps2 pl) BARRIER Sid|Kvm OR CLBC);

# substantiiv või pronoomen mitmuse nominatiivis ei saa olla subjektiks,
# kui osalauses leidub verb ainsuses
REMOVE (S pl nom @SUBJ) OR (P pl nom @SUBJ)
               (*-1 (sg <FinV>) BARRIER Sid|Kvm OR CLBC);
REMOVE (S pl nom @SUBJ) OR (P pl nom @SUBJ)
               (*1 (sg <FinV>) BARRIER Sid|Kvm OR CLBC);
REMOVE (sg nom @SUBJ) (*-1 (pl <FinV>) BARRIER Sid|Kvm OR CLBC LINK NOT 0 (sg))
               (NEGATE *1 (@<Q) BARRIER CLBC)(NOT 1 Sid|Kvm); # TP +NOT0sg
REMOVE (sg nom @SUBJ) (*-1 (pl <FinV>) BARRIER Sid|Kvm OR CLBC)
               (NEGATE *1 (@<Q) BARRIER CLBC)(1 Koma)(NEGATE *1 (S) BARRIER CLBC);

# sõna partitiivis ei ole subjekt, kui osalauses leidub verb mitmuses
REMOVE (part @SUBJ) (*-1C (V pl) BARRIER CLBC) ;
REMOVE (part @SUBJ) (*1C (V pl) BARRIER CLBC) ;
# osastavas käändes nimi-asesona ei ole subjekt, kui osalause verb on esimeses voi teises pöördes.
REMOVE (part @SUBJ) (*-1C (V ps1 sg) BARRIER CLB);
REMOVE (part @SUBJ) (*-1C (V ps1 pl) BARRIER CLB);
REMOVE (part @SUBJ) (*-1C (V ps2 sg) BARRIER CLB);
REMOVE (part @SUBJ) (*-1C (V ps1 pl) BARRIER CLB);
# infinitiivi vorm saab olla subjekt üksnes siis, kui öeldiseks on verb
# 3. pöörde mitmuses, seega infinitiivi vorm ei ole subjekt, kui osalauses
# leidub verbe 1., 2. pöördes või 3. pöörde mitmuses
REMOVE (inf @SUBJ) (*-1C (V ps1) OR (V ps2) OR (V ps3 pl) BARRIER CLBC);
REMOVE (inf @SUBJ) (*1C (V ps1) OR (V ps2) OR (V ps3 pl) BARRIER CLBC);

# kui "mina", "meie", "teie" ühildub öeldisega, siis ta on alus
SELECT ("mina" sg nom @SUBJ) OR ("ma" sg nom @SUBJ) (*-1 (V ps1 sg) BARRIER CLB);
SELECT ("mina" sg nom @SUBJ) OR ("ma" sg nom @SUBJ) (*1 (V ps1 sg) BARRIER CLB);
SELECT ("mina" pl nom @SUBJ) OR ("ma" pl nom @SUBJ) (*-1 (V ps1 pl) BARRIER CLB);
SELECT ("mina" pl nom @SUBJ) OR ("ma" pl nom @SUBJ) (*1 (V ps1 pl) BARRIER CLB);
SELECT ("sina" pl nom @SUBJ) OR ("sa" pl nom @SUBJ) (*-1 (V ps2 pl) BARRIER CLB);
SELECT ("sina" pl nom @SUBJ) OR ("sa" pl nom @SUBJ) (*1 (V ps2 pl) BARRIER CLB);

# 3) Unikaalsus
# kui osalauses leidub ühene subjekt, siis rohkem subjekte olla ei saa,
# välja arvatud koordineeritud subjektid
REMOVE:test (@SUBJ) (*-1C (@SUBJ) BARRIER Sid|Kvm OR CLBC);
# Vead tekivad selletõttu, et kuskil midagi valesti määratud.
# Töömees on _eestlane_ küll, ...
# nad hoidsid teda ka nagu minu _vanemad_ mind
REMOVE (@SUBJ) (*1C Subjekt BARRIER Sid|Kvm OR CLBC)(NOT 0 (prop));
REMOVE (@SUBJ) (*-1C Subjekt BARRIER CLB LINK *1 FinV BARRIER Sid|Kvm);
# Langend Patroklos ja keeb tema ümber nüüd metsikuim _võitlus_
REMOVE (@SUBJ) (*1C Subjekt BARRIER CLBC LINK *-1 FinV BARRIER Sid|Kvm);

# 4) Lause ehitusest soltuvad reeglid

# kui kõrvuti paiknevad kaks nime nominatiivis ja mõlemad võivad olla
# subjektiks, siis esimene ei ole subjekt
REMOVE (@SUBJ) (0 (nom))(1C (prop nom))(1 Subjekt)(NOT 0 (P indef) OR (P pers));
# substantiiv või pronoomen partitiivi mitmuses on subjekt, kui osalauses
# pole muud ühest subjekti ja öeldiseks on verb "olema"
SELECT:kahtlane (S pl part @SUBJ) OR (P pl part @SUBJ) 
               (*-1 ("ole") BARRIER Verb OR CLB)
               (NEGATE *1 Verb BARRIER CLB)
               (NEGATE *0 Subjekt BARRIER CLB)
               (NEGATE *-1 NimKv OR KvAdv OR ("üks") BARRIER CLB);
# Viga tekkis enne: et on _teisigi_ Partei vaenlasi

# sõna partitiivis pole subjekt, kui talle järgneb da-infinitiivne öeldis
REMOVE (part @SUBJ) (1 Inf)(1C Oeldis); # TP part viidud Subj juurde, oli 0-ting
# kui kõrvuti on nominatiivis ase- ja nimisõna, siis asesõna ei ole subjekt
REMOVE (P sg nom @SUBJ) (1 (S sg nom))(NOT -1 ("ole"))(NOT 0 (P pers));
REMOVE (P sg nom @SUBJ) (1 (S sg nom))(-1 ("ole"))
               (*1 (V partic) BARRIER CLBC)(NOT 0 (P pers)); 
# Kuid praegusel hetkel oli _see_ monoloogki katkenud.
# Aga: Veel enam aga oli see enesehüpnoos.
# Aga: Kui _sa_ siga alla ei anna, siis ..

# sõna partitiivis pole subjekt, kui öeldis on sellest sõnast paremal ja
# öeldisest paremal osalauses leidub nimi- või asesõna nominatiivis
# Kombinatsioon Subj(part) Fv Obj(nom,gen) esineb va"ga harva
REMOVE (part @SUBJ) (NEGATE *-1 Oeldis BARRIER CLB)
               (*1C Oeldis BARRIER (Z) OR CLB)
               (*1 (S nom) OR (P nom) BARRIER CLB LINK NOT 0 Aeg);

# da-infinitiivid
SELECT (inf @SUBJ) (NEGATE *-1 Inf BARRIER CLB)
           (NEGATE *1 Inf BARRIER CLB)
           (*-1 Suutelisusv OR Emotsioonv OR Sobivusv BARRIER CLB LINK 0C Oeldis);
SELECT (inf @SUBJ) (NEGATE *-1 Inf BARRIER CLB)
           (NEGATE *1 Inf BARRIER CLB)
           (*1 Suutelisusv OR Emotsioonv OR Sobivusv BARRIER CLB LINK 0C Oeldis);
SELECT (inf @SUBJ) (NEGATE *-1 Inf BARRIER CLB)
               (NEGATE *1 Inf BARRIER CLB)
               (*-1 Vajadusv BARRIER CLB OR (S nom) OR (P nom) LINK 0C Oeldis);
# et vältida: tuli mõte tegema hakata
SELECT (inf @SUBJ) (NEGATE *-1 Inf BARRIER CLB)
               (NEGATE *1 Inf BARRIER CLB)
               (*1 Vajadusv BARRIER CLB LINK 0C Oeldis);

SELECT (inf @SUBJ) (*-1 Vaja BARRIER CLBC);
SELECT (inf @SUBJ) (*1 Vaja BARRIER CLBC);

SELECT (inf @SUBJ) (*-1 Sid|Kvm BARRIER (CLB) LINK *-1C (inf @SUBJ) BARRIER (CLB));
SELECT (inf @OBJ) (*-1 Sid|Kvm BARRIER (CLB) LINK *-1C (inf @OBJ) BARRIER (CLB));




# da-infinitiiv on subjekt, kui osalauses: öeldiseks on "olema" (üksinda),
# leidub predikatiiv ja puudub nimi- või asesõnaline nominatiivne subjekt
SELECT (inf @SUBJ) (*-1C ("ole" main <FinV>) BARRIER CLB)
               (*-1C (@PRD) BARRIER CLB)
               (NEGATE *-1 (S nom) OR (P nom) BARRIER CLB)
               (NEGATE *1 (S nom) OR (P nom) BARRIER CLB);
#... aga voimatu oli seda täiesti välja lülitada
#... muidugi oli täiesti voimatu ütelda

# da-infinitiiv on subjekt, kui osalauses esineb järgmine konstruktsioon:
# eelmine sõna on predikatiiv nominatiivis, üleelmine on "olema" ning
# osalauses vasakul ei esine muid sõnu nominatiivis ega partitiivis

SELECT (inf @SUBJ) (-2 ("ole"))(-1C (@PRD))(-1 (nom))
               (NEGATE *-2 (nom) BARRIER CLB)(NEGATE *-1 (part) BARRIER CLB);
# Mul on raske uskuda vrs Laud on raske tosta
# da-infinitiiv pole subjekt, kui lauses leiduvad nii nimi- voi asesona nimetavas kui osastavas käändes
REMOVE (inf @SUBJ) (*-1 (S sg nom) OR (P sg nom) BARRIER CLB OR Verb LINK *1C (S part) OR (P part));

# Nimetavas käändes sona on subjekt, kui lauses on ainult üks nominatiiv ja verb ei ole impersonaal jms.
SELECT (nom @SUBJ) (NOT 0 Arv OR Verb)
               (NOT *0 Koma OR ("ja") OR ("ning") OR (S nom) OR (P nom) OR 
                        (inf @SUBJ) OR Impl OR Imper OR (V ps1) OR (V ps2))
               (NEGATE *1 (N card nom) BARRIER CLB LINK 1 (sg part) LINK NOT 0 Aeg)
               (NOT 1 Inf);
SELECT (nom @SUBJ) (NOT 0 Arv)(*0 Verb BARRIER CLB)
               (NOT *-1 Koma OR ("ja") OR ("ning") OR (S nom) OR (P nom) OR 
                        (inf @SUBJ) OR Impl OR Imper OR (V ps1) OR (V ps2))
               (NOT *1 Koma OR (S nom) OR (P nom) OR 
                        (inf @SUBJ) OR Impl OR Imper OR (V ps1) OR (V ps2))
               (NEGATE *1 (N card nom) BARRIER CLB LINK 1 (sg part) LINK NOT 0 Aeg)
               (NOT 1 Inf);
# Viga: Või siis kusagilt tihnikust ohvrile vibunool kõrri lasta
SELECT (nom @SUBJ) (NOT 0 Arv OR (prop))
         (NEGATE *-1 Koma OR ("Cpr") OR ("ja") OR ("ning") OR SbPrNom OR 
         (inf @SUBJ) OR Impl OR Imper OR (V ps1) OR (V ps2) BARRIER CLB) 
         (NEGATE *1 SbPrNom OR (inf @SUBJ) OR Impl OR Imper OR 
                         (V ps1) OR (V ps2) BARRIER CLB)
         (*1 Koma BARRIER (S) OR (P) OR CLB)(*-1 FinV BARRIER CLB)
         (NOT -1 Partic)(NOT 1 Inf); 
SELECT (prop nom @SUBJ) (NOT 1 (prop))
               (NEGATE *-1 Koma OR ("ja") OR ("ning") OR SbPrNom OR 
               (inf @SUBJ) OR Impl OR Imper OR (V ps1) OR (V ps2) BARRIER CLB)
               (NEGATE *1 SbPrNom OR (inf @SUBJ) OR Impl OR Imper OR 
                         (V ps1) OR (V ps2) BARRIER CLB)
               (*1 Koma BARRIER (S) OR (P) OR CLB)(*-1 FinV BARRIER CLB)
               (NOT -1 Partic)(NOT 1 Inf);      
# vaid on unustanud _kõik_ selle

LIST Vps12 = (V ps1) (V ps2);

SELECT (nom @SUBJ) (NOT 0 Arv)(*1 Koma BARRIER CLBC LINK NOT *1 (S) OR (P) BARRIER CLBC)
               (NEGATE *-1 Koma OR Ja OR SbPrNom OR (inf @SUBJ) OR Impl OR Imper
		 OR Vps12 BARRIER CLBC)
               (NEGATE *1 SbPrNom OR (inf @SUBJ) OR Impl OR Imper OR Vps12
                   BARRIER CLBC)
               (*1 (@FMV) OR (@FCV) BARRIER CLBC)(NOT 1 Inf);
SELECT (sg nom @SUBJ) (1C (sg ps3))
    (NOT *-1 Koma OR Ja OR SbPrNom OR (inf @SUBJ) OR Impl OR Imper OR Vps12)   
    (NOT *1 Koma OR SbPrNom OR (inf @SUBJ) OR Impl OR Imper OR Vps12);

SELECT (S nom @SUBJ) 
   (NEGATE *-1 Ja OR SbPrNom OR (inf @SUBJ) OR Impl OR Imper OR Vps12 BARRIER CLBC)
   (NEGATE *1 SbPrNom OR (inf @SUBJ) OR Impl OR Imper OR Vps12 BARRIER CLBC)
   (*-1 (@FMV) ^ Prverb OR (@FCV) ^ Prverb BARRIER CLBC)
   (NOT 1 Inf)(NOT -1 Võrdlus) ;

#Vead:
#Sinu numbrid on juba nii mustad, et ajavad juuksed peas püsti.
#Lennartil läheb punast marlborot sigaret sigareti järel
#Kas ei saaks tund aega
SELECT (S nom @SUBJ) 
               (NEGATE *-1 Ja OR SbPrNom OR (inf @SUBJ) OR Impl OR Imper OR
		 Vps12 BARRIER CLB)
               (NEGATE *1 SbPrNom OR (inf @SUBJ) OR Impl OR Imper OR Vps12
		 BARRIER CLB)
               (*1 (@FMV) OR (@FCV) BARRIER CLB)(NOT 1 Inf);

# Viga: Sõna Ingsots kord ilmus, kord kadus.

SELECT (nom @SUBJ) (NOT 0 (@ADVL))(NOT 1 (prop))
               (*-1 FinV ^ Impl BARRIER CLB LINK NOT -1 (J))
               (NEGATE *0 Subjekt OR Tud OR Imper OR Inf BARRIER CLB);

#Viga: Viisid minema kõik kuld- ja hõberiistad.

# Kvantoradverb on subjekt, kui pole nimetavas käändes nimi- ega asesonu ega
# subjekti olemasolu välistavaid verbivorme.


LIST SPNomPar = (S nom) (S par) (P nom) (P par);
LIST NomPar = nom par;

SELECT (@SUBJ) (0 KvAdv)(1C (@<Q))
               (NOT *-1 Koma OR SPNomPar OR (inf @SUBJ) OR Impl OR Imper OR Ja)
               (NOT *1 Koma OR SPNomPar OR (inf @SUBJ) OR Impl OR Imper);
SELECT (@SUBJ) (0 KvPron + NomPar)(1C (@<Q))
               (NOT *-1 Koma OR SPNomPar OR (inf @SUBJ) OR Impl OR Imper OR Ja)
               (NOT *1 Koma OR SPNomPar OR (inf @SUBJ) OR Impl OR Imper);

SELECT (nom @SUBJ) (NOT 0 Arv)
               (NOT *-1 Koma OR SPNomPar OR Subjekt OR Impl OR Imper OR
                Ja OR Vps12)
               (NOT *1 Koma OR SPNomPar OR Subjekt OR Impl OR Imper OR Vps12)
               (NOT 1 Inf);
 
SELECT (nom @SUBJ) (-1 Neg)(1 Partic);

SELECT (pl nom @SUBJ) (1 (V pl ps3)) (*1 (sup in) BARRIER CLB);
SELECT ("see" nom @SUBJ) (1 ("ole"))(*1 SbPrNom BARRIER CLBC);
SELECT ("kes" @SUBJ) (-1 Koma)(NOT *1 Impl);

# Arvsõnad
# Arv nimetavas käändes on subjekt, kui verb on mitmuse 3. pöördes ja osalauses
# pole mitmuse nimetavat käänet.
SELECT (N nom @SUBJ) (NOT 0 ("üks"))(*-1C (V ps3 pl) BARRIER CLB)
                  (NEGATE *-1 (pl nom) OR Impl BARRIER CLB)
                  (NEGATE *1 (pl nom) OR Impl BARRIER CLB);
SELECT (N nom @SUBJ) (NOT 0 ("üks"))(*1C (V ps3 pl) BARRIER CLB)
                  (NEGATE *-1 (pl nom) OR Impl BARRIER CLB)
                  (NEGATE *1 (pl nom) OR Impl BARRIER CLB);
# 5) Koordinatsioon
# kui sõnade "nii" ja "kui ka" abil koordinatsioonis olevatest sõnadest üks
# ei ole subjekt, siis ei ole seda ka teine
REMOVE (@SUBJ) (-1 Nii)(1 ("kui"))(2 Ka)(NOT 3 Subjekt)(4 Verb);
REMOVE (@SUBJ) (-4 Nii)(NOT -3 Subjekt)(-2 ("kui"))(-1 Ka)(1 Verb);
# kui mitmuse partitiivis nimi- või asesõna on on koordineeritud (sidesõnaga)
# samas vormis ühese (kindla) subjektiga, siis ta on ka subjekt
SELECT (@SUBJ) (-2 SbPr + (pl part))(-2C Subjekt)
               (-1 JaVoi)(0 SbPr + (pl part))(1 CLB);
# kui fraas on tüüpi "suured apelsinid, väikesed õunad" ja "apelsinid" on
# subjekt, siis ka "õunad on subjekt"
SELECT (@SUBJ) (-4 (A pl nom))(-3 (S pl nom))(-2 Koma)
               (-1 (A pl nom))(0 (S pl nom))(-3C Subjekt);
SELECT (S pl nom @SUBJ) (-1 Adj)(-2 JaVoiK)(-3 (pl nom))(-3C Subjekt);
SELECT (S pl nom @SUBJ) (-1 Eestäiend)(-2 Eestäiend)(-3 JaVoiK)
               (-4 (pl nom))(-4C Subjekt);
SELECT (pl nom @SUBJ) (NOT 0 (P det))(-1 JaVoi ^ CLB)(-2 (pl nom))(-2C Subjekt);
SELECT (pl nom @SUBJ) (1 JaVoi ^ CLB)(2 (pl nom))(2C Subjekt) ;
SELECT (sg nom @SUBJ) (-1 JaVoi ^ CLB)(-2 (sg nom))(-2C Subjekt) ;
SELECT (sg nom @SUBJ) (1 JaVoi ^ CLB)(2 (sg nom))(2C Subjekt) ;

#6) Määrsonalised kvantorid
# määrsõnaline kvantor ei saa olla subjekt, kui paremas kontekstis
# ei leidu kvantori võimalikku laiendit partitiivis
REMOVE (D @SUBJ) (NOT 1 Adv OR (part));
REMOVE (D @SUBJ) (1 Adv)(NOT 2 (part));

#7) Asesona ise
REMOVE (S @SUBJ) OR (P @SUBJ) (NOT 0 (nom) OR (part));
REMOVE (@SUBJ) (0 Ise);

#8) Sona on sulgudes
SELECT (Y @SUBJ) (-1 (Opar))(1 (CPar))(-2C Subjekt);
REMOVE (Y @SUBJ) (-1 (OPar))(1 (CPar))(NOT -2 Subjekt);
# kui sõna taga on seda tähendav lühend, siis loetakse et nad on
# koordinatsioonis nagu ehk
REMOVE (@SUBJ) (*-1 (OPar) BARRIER (CPar) OR FinV LINK *1 SbPr BARRIER (CPar))
               (*1 (CPar) BARRIER (OPar) OR FinV);
REMOVE (@SUBJ) (*-1 (OPar) BARRIER (CPar) OR FinV )
               (*1 (CPar) BARRIER (OPar) OR FinV LINK *-1 SbPr BARRIER (OPar));

# kuid sona ei tohi olla seal üksi
# need on enamasti järellisandid

# III Objektigrammatika

# 1) Teatud liiki öeldised pohjustavad da-infintiivse objekti
SELECT (inf @OBJ) (*-1 (<InfP>) ^ Inf BARRIER CLB LINK NOT 1 JaVoi);
SELECT (inf @OBJ) (*1 (<InfP>) BARRIER Koma OR CLB);
SELECT (inf @OBJ) (1 (A partic past));
REMOVE (inf @OBJ) (NEGATE *-1 (<InfP>) BARRIER CLBC)
              (NEGATE *1 (<InfP>) BARRIER CLB);

# 2) öeldise vorm voi liik välistab objekti
# a) öeldis on intransitiivne
REMOVE (@OBJ) (NEGATE *-1 Verb BARRIER CLB)(NOT 1 Partic)
              (*1C Intr&YV BARRIER Verb ^ Intr&YV ^ EiAra OR CLB 
                   LINK NEGATE *1 Verb BARRIER CLBC);

REMOVE (@OBJ) (NEGATE *1 Verb BARRIER CLB)(NOT 1 Partic)
              (*-1C Intr&YV BARRIER Verb ^ Intr&YV ^ EiAra OR CLB 
                    LINK NEGATE *-1 Verb BARRIER CLB); 

# Vead: 
# Nagu võiks mõnda teist inimest, kes ei ole nii arukas, sellega ära petta.
# Ta tegi, ..., esikupeegli ees ... grimassi
# Ta võttis pakist, kus oli ..., ühe sigareti.

REMOVE (@OBJ) (NEGATE *1 Verb BARRIER CLB)(NOT 1 Partic)
              (*-1C Intr&YV BARRIER Verb ^ Intr&YV ^ EiAra OR CLB 
                    LINK -1 Ei LINK NEGATE *-1 Verb BARRIER CLB); 

REMOVE (@OBJ) (NOT 1 Partic)
              (*-1C Intr&YV BARRIER Verb ^ Intr&YV ^ EiAra OR CLB 
                    LINK NEGATE *-1 Verb BARRIER CLB)
              (*1C Intr&YV BARRIER Verb OR CLB LINK NEGATE *1 Verb BARRIER CLB); 

REMOVE (@OBJ)(0 (sg nom))(*1C Intr&YV + main BARRIER Verb ^ Intr&YV ^ EiAra OR CLBC)
             (NEGATE *1 Partic OR Inf BARRIER CLBC)(NEGATE *-1 Verb BARRIER CLBC);

# see oli olnud ...
REMOVE (@OBJ) (NEGATE *-1 Verb BARRIER CLB)
              (*1C Intr&YV BARRIER CLB OR Verb ^ Intr&YV ^ EiAra 
                   LINK *1C Intr&YV BARRIER CLB LINK NEGATE *1 Verb BARRIER CLB) ;
# Viga: Sammu sõitma hakkasime alles siis, ...

#oli see olnud ...
REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB LINK NEGATE *-1 Verb BARRIER CLB)
              (*1C Intr&YV BARRIER CLB LINK NEGATE *1 Verb BARRIER CLB);
REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB LINK -1  Ei LINK NEGATE *-1 Verb BARRIER CLB)
              (*1C Intr&YV BARRIER CLB LINK NEGATE *1 Verb BARRIER CLB);
# ei olnud säilinud midagi
# jäi korraks õhku rippuma nagu porikärbes
REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB OR Verb ^ Intr&YV ^ EiAra
                    LINK *-1C Intr&YV BARRIER CLB 
                    LINK NEGATE *-1 Verb BARRIER CLB)
              (NEGATE *1 Verb BARRIER CLB);

REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB
                    LINK *-1C Intr&YV BARRIER CLB
                    LINK -1 Ei 
                    LINK NEGATE *-1 Verb BARRIER CLB)
              (NEGATE *1 Verb BARRIER CLB) ;
REMOVE (@OBJ) (*-1C Intr&YV BARRIER CLB OR Verb ^ Intr&YV ^ EiAra
                    LINK *-1C Intr&YV BARRIER CLB OR Verb ^ Intr&YV ^ EiAra
                    LINK NEGATE *-1 Verb BARRIER CLB)
              (*1 Kvm BARRIER Verb OR CLB);

#b) öeldis on eitavas kones
LIST NimOm = nom gen;
LIST Kysi = Int ;
LIST Nom = nom ;
LIST Gen = gen ;
LIST Par = part;
LIST Tr = tr;
LIST Mis = "mis";
LIST Koik = "kõik";

REMOVE (@OBJ) (0 NimOm)(NEGATE *1 Kysi OR Tud BARRIER CLB)
              (*0 Ei BARRIER CLB)
              (NEGATE *-1 Tud BARRIER CLB);
REMOVE (gen @OBJ) (NEGATE *1 Kysi BARRIER CLB)(*0 Ei BARRIER CLB);


#c) Isikuline tegumood välistab ainsuse nominatiivse objekti
REMOVE (sg nom @OBJ) (NEGATE *-1 Impl OR Imper OR InfSupiin BARRIER CLBC)
              (NEGATE *1 Impl OR Imper OR InfSupiin BARRIER CLBC)
              (NOT 0 Arv OR Mis OR Koik);
# Viga: Palun see suurem sealt

#d) Umbisikuline tegumood voi käskiv koneviis välistab genitiivse objekti
REMOVE (gen @OBJ) (*-1C Impl BARRIER CLBC);
REMOVE (gen @OBJ) (*1C Impl BARRIER CLBC);
REMOVE (gen @OBJ) (*-1C Imper BARRIER CLBC);
REMOVE (gen @OBJ) (*1C Imper BARRIER CLBC);

#e) Verb nouab partitiivset objekti

LIST PartObjVerb = <Part> <Part-P>;
LIST Ära = "ära";

SELECT (@OBJ) (*-1 PartObjVerb BARRIER CLBC)
              (0 Par ^ Aeg)
              (NEGATE *-1 (part @OBJ) OR Ära BARRIER CLBC )
              (NEGATE *1 (part @OBJ) OR Ära BARRIER CLBC);
#Viga: Kui ülempreester ei tahtnud loomulikult _sellist_ rahakat ning -...

SELECT (@OBJ) (*1 PartObjVerb BARRIER CLBC)
              (0 Par ^ Aeg)
              (NEGATE *0 (part @OBJ) OR Ära BARRIER CLBC );
              

REMOVE (@OBJ) (NOT 0 Par OR Inf OR Pl)
              (*-1 PartObjVerb + (ps) BARRIER CLB OR Verb ^ PartObjVerb 
                   LINK NOT *-1 Verb BARRIER CLB)
              (NEGATE *1 Verb BARRIER CLB)
              (NEGATE *0 Tr OR Ära BARRIER CLB)
              (NOT 1 Partic) ;

REMOVE (@OBJ) (NOT 0 Par OR Inf OR Pl)
              (*1 PartObjVerb + (ps) BARRIER CLB OR Verb ^ PartObjVerb 
                   LINK NOT *1 Verb BARRIER CLB)
              (NEGATE *-1 Verb BARRIER CLB)
              (NEGATE *0 Tr OR Ära BARRIER CLB)
              (NOT 1 Partic) ;

#3) Erijuhud

# Kui objekt ja subjekt on samas vormis, siis tavaliselt subjekt eelneb objektile
REMOVE (@OBJ) (0 (@SUBJ pl nom))
              (NEGATE *-1 Subjekt BARRIER CLB)
              (*1 FinV BARRIER CLB LINK *1 (@OBJ pl nom) BARRIER Sid|Kvm OR CLB);
# Esimeses kahes isikus isikulised asesonad ei ole nom objektid 
REMOVE (@OBJ) (0 (P pers ps1 nom) OR (P pers ps2 nom));
# kui osalauses leidub kaks sona, mis voivad olla nii objektiks kui ka
# subjektiks, üks nominatiivne ja teine partitiivne, siis nominatiivne ei ole
# objekt
LIST AtrAdv = @NN> @<NN @ADVL;
REMOVE (@OBJ) (0 (@SUBJ nom))(NOT 1 Partic)
              (NEGATE *0 InfSupiin BARRIER CLB)
              (*1 (@OBJ part) ^ AtrAdv BARRIER CLB);

REMOVE (@OBJ) (0 (@SUBJ nom))(NOT 0 ("mis"))(NOT 1 Partic)
              (NEGATE *0 InfSupiin BARRIER CLB)
              (*1 (@OBJ pl nom) BARRIER Sid|Kvm OR CLB);
              
              
# kombinatsioon Obj(nom/gen) Fv Subj(nom) ei ole võimalik
#KONTROLLI!
REMOVE (gen @OBJ) (NOT 1 Oeldis OR CLB)
              (NEGATE *-1 Oeldis OR (V partic) BARRIER CLB)
              (*2C Oeldis BARRIER CLB LINK *1 SbPrNom BARRIER Kvm OR CLB);

REMOVE (sg nom @OBJ) (NOT 0 Mis)(NEGATE *1 Ise BARRIER CLB)
              (NEGATE *-1 Oeldis BARRIER CLB)
              (*1C Oeldis BARRIER CLB LINK *1 SbPrNom BARRIER Sid|Kvm OR CLB);
# On voimalik: ta sai aru, kui suure _asja_ ta on ette votnud
# Juba tempel ise on püstitatud ...

#Arvsõnad ei ole genitiivsed objektid
REMOVE (N gen @OBJ) (NOT 0 ("üks"));
#Tud-vormile ei eelne gen objekti
REMOVE (gen @OBJ) (1 Tud);
#      Nt tema lukustatud kabinetis

LIST NudTudDes = (partic past) ger <nud> <tud>;

#Kui nud-, tud- v_i des-vormile eelneb vahetult partitiiv , siit ta on objekt
SELECT (@OBJ) (0 Par)(1 NudTudDes ^ Intr&YV)(NOT 0 (@<P) OR Aeg OR (P dem))
              (NEGATE *-1 Neg BARRIER CLB);

SELECT (P dem part @OBJ) (1 NudTudDes ^ Intr&YV)(NOT 0 (@<P) OR Aeg)
              (NEGATE *-1 Neg BARRIER CLB)(*1 NPpiir BARRIER Par OR CLB);

# Osastavas käändes sõna enne partitsiipi on suure tõenäosusega sihitis,
# v.a. määrused
SELECT (S part @OBJ) (NOT 0 (@ADVL))(1 Partic ^ Intr&YV);

# Kui lauses pole verbi, ei ole ka nominatiivset objekti
REMOVE (nom @OBJ) (NOT *1 Verb)(NOT *-1 Verb);

#Kaassonafraasis ei ole objekti
REMOVE (@OBJ) (*-1 (pre) BARRIER (@<P) OR CLB)
              (*1 (@<P) BARRIER (pre) OR CLB);
# Kui omastav ja osastav kääne on korvuti ja molemad voivad olla objektid, siis
# omastav ei ole objekt, v.a erandid
REMOVE (gen @OBJ) (1 (@OBJ part))(NOT 1 (@Q>) OR (@<P) OR (@P>))
              (NOT 2 Partic OR InfSupiin);
# Viga: Ta võttis südame rindu

REMOVE ("oma" @OBJ) (1 (@OBJ part))(NOT 2 Partic OR InfSupiin);
REMOVE (gen @OBJ) (1 (A part))(2 (@OBJ part))(NOT 3 Partic OR InfSupiin);
REMOVE ("oma" @OBJ) (1 (A part))(2 (@OBJ part))(NOT 3 Partic OR InfSupiin);

#konditsionaal (pole arvu)
REMOVE (nom @OBJ) (0 (@SUBJ))(1 (cond))(*1 Objekt BARRIER CLB); 



#4) Unikaalsus
# kui osalauses ei leidu partitsiipe, gerundiive, supiine ega infinitiive
# ning leidub kindel objekt, kusjuures ei leidu rinnastavaid komasid ega
# sidesonu,  siis osalauses ei saa olla rohkem objekte

LIST SupInfGer = sup inf ger;

REMOVE (@OBJ)(NOT 0 Inf)
             (NEGATE *0 Partic OR SupInfGer BARRIER CLB)
             (*0C Objekt BARRIER Sid|Kvm OR CLB) ;
#Viga tekkis siis, kui eelnevalt valesti määratud:
#Laseks ta nooli täis nagu Püha Sebastianuse

#või vaeglauses
#nemad hoidsid teda ka nagu minu vanemad mind
#pani ka noor kirjanik nõukogudemaad kiitvaid lauseid paberile

#riik sai miljardeid kroone kahjumit 


LIST Aux = aux;

REMOVE (@OBJ)(*-1 Aux BARRIER CLB)
             (*1 Partic BARRIER CLB)
             (*1C Objekt BARRIER Sid|Kvm OR CLB)
             (NEGATE *0 SupInfGer BARRIER CLB);
REMOVE (@OBJ)(*-1 Aux BARRIER CLB)
             (*-1 Partic BARRIER CLB)
             (*-1C Objekt BARRIER Sid|Kvm OR CLB)
             (NEGATE *0 SupInfGer BARRIER CLB);
REMOVE (@OBJ)(*1 Aux BARRIER CLB)
             (*1 Partic BARRIER CLB)
             (*1C Objekt BARRIER Sid|Kvm OR CLB)
             (NEGATE *0 SupInfGer BARRIER CLB);

REMOVE (@OBJ)(*-1C FinV BARRIER CLB)
             (*1C Objekt BARRIER Sid|Kvm OR CLB)
             (NEGATE *0 SupInfGer BARRIER CLB);             
             
# 5) Koordinatsioon
# kui koordinatsioonis olevatest samas vormis sonadest üks ei ole objekt,
# siis ei ole seda ka teine
REMOVE (@OBJ) (-1 Nii)(1 Kui)(2 Ka)(NOT 3 Objekt)(4 Verb);
REMOVE (@OBJ) (-4 Nii)(NOT -3 Objekt)(-2 Kui)(-1 Ka)(1 Verb);

REMOVE (S sg nom @OBJ) OR (P sg nom @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (sg nom))(NOT 2 Objekt OR Eestäiend);
REMOVE (S pl nom @OBJ) OR (P pl nom @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (pl nom))(NOT 2 Objekt OR Eestäiend);
REMOVE (S sg gen @OBJ) OR (P sg gen @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (sg gen))(NOT 2 Objekt OR Eestäiend);
REMOVE (S sg part @OBJ) OR (P sg part @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (sg part))(NOT 2 Objekt OR Eestäiend);
REMOVE (S pl part @OBJ) OR (P pl part @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (pl part))(NOT 2 Objekt OR Eestäiend);
REMOVE (S sg nom @OBJ) OR (P sg nom @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (sg nom))(NOT -2 Objekt);
REMOVE (S pl nom @OBJ) OR (P pl nom @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (pl nom))(NOT -2 Objekt);
REMOVE (S sg gen @OBJ) OR (P sg gen @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (sg gen))(NOT -2 Objekt);
REMOVE (S sg part @OBJ) OR (P sg part @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (sg part))(NOT -2 Objekt);
REMOVE (S pl part @OBJ) OR (P pl part @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (pl part))(NOT -2 Objekt);

SELECT (S sg nom @OBJ) OR (P sg nom @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (sg nom))(2C Objekt);
SELECT (S pl nom @OBJ) OR (P pl nom @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (pl nom))(2C Objekt);
SELECT (S sg gen @OBJ) OR (P sg gen @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (sg gen))(2C Objekt);
SELECT (S sg part @OBJ) OR (P sg part @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (sg part))(2C Objekt);
SELECT (S pl part @OBJ) OR (P pl part @OBJ) (1 JaVoi ^ CLBC)
              (2 SbPr + (pl part))(2C Objekt);
SELECT (S sg nom @OBJ) OR (P sg nom @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (sg nom))(-2C Objekt);
SELECT (S pl nom @OBJ) OR (P pl nom @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (pl nom))(-2C Objekt);
SELECT (S sg gen @OBJ) OR (P sg gen @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (sg gen))(-2C Objekt);
SELECT (S sg part @OBJ) OR (P sg part @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (sg part))(-2C Objekt);
SELECT (S pl part @OBJ) OR (P pl part @OBJ) (-1 JaVoi ^ CLBC)
              (-2 SbPr + (pl part))(-2C Objekt);

# 6) Määrsonalised kvantorid objektina
# määrsona ei ole objekt, kui osalauses paremal ei leidu sonu partitiivis
# voi partitiiivis sona ei ole selle määrsona (kvantori) laiendiks
REMOVE (D @OBJ) (NOT 1 Adv)(NOT 1 Par)(NOT 1 Eestäiend);
REMOVE (D @OBJ) (1 Eestäiend)(NOT 1 Par)(NOT 1 Adv)(NOT 2 Par);
REMOVE (D @OBJ) (1 Adv)(NOT 2 Par);

#7)Asesona ise
REMOVE (P @OBJ) (NOT 0 NGP);

#8) Sona on sulgudes
REMOVE (@OBJ) (*-1 OPar BARRIER CPar OR Oeldis LINK NOT -1 Objekt)
              (*1 CPar BARRIER OPar OR Oeldis);
              
              

#IV PREDIKATIIVIGRAMMATIKA

LIST Ger = ger;

#1) Osalauses on leitud öeldise osa, mis ei ole predikatiivi lubav
REMOVE (@PRD) (*-1C IFV ^ Prverb BARRIER CLBC OR Kvm);
REMOVE (@PRD) (*1C IFV ^ Prverb BARRIER CLBC)(NEGATE *1 (Dsh) BARRIER CLBC)
              (NOT 1 Koma);
# Viga: See ei olnud siiski _ketserlus_, mis oli kirjutatud tema näkku, vaid
# hoopis intelligents
REMOVE (@PRD) (NOT 0 Nom)(*-1C IFV ^ ("ole") BARRIER CLBC);
REMOVE (@PRD) (NOT 0 Nom)(*1C IFV ^ ("ole") BARRIER CLBC);
REMOVE (@PRD) (*-1C (@FMV) ^ Prverb BARRIER Kvm OR CLBC) 
              (NEGATE *0 Prverb BARRIER CLBC);
REMOVE (@PRD) (*1C (@FMV)^ Prverb BARRIER CLBC)
              (NEGATE *0 Prverb BARRIER CLBC);

REMOVE (@PRD) (*-1 Koma BARRIER CLB LINK 1 Ger ^ Prverb)
              (NEGATE *1 Prverb BARRIER CLB);
# olevat Antti seejuures üsna jämedalt väljendunud, kasutades koige 
# ürgsemat _sorti_ soome-ugri sonavara

# 2) Kui lauses leidub sonadekombinatsioon on vaja, ei ole predikatiivi 
REMOVE (@PRD) (*-1 Vaja BARRIER CLBC);
REMOVE (@PRD) (*1 Vaja BARRIER CLBC);

SELECT (nom @PRD) (1 ("ole") + Inf + main)(*-1 Subjekt BARRIER CLB);
SELECT (A nom @PRD) (1 ("ole") + Inf + main)(1 Subjekt);

#3) Sonade järjestus ei luba predikatiivi
LIST JärelKv = @<Q;
LIST ParticPast = (partic past);

REMOVE (nom @PRD) (1 Subst + Nom)(NOT 1 JärelKv);
REMOVE (@PRD) (0 Par)(1 Subst + Par)(NOT 1 JärelKv);
REMOVE (@PRD) (0 ParticPast)(1 InfSupiin);
REMOVE (@PRD) (0 ParticPast)(-1 (sup));


#4) Adjektiine öeldistäide soltub nominatiivsete substantiivide olemasolust
SELECT (A nom @PRD) (*-1 ("ole") BARRIER Subst + Nom)(NOT *1 Subst + Nom);
REMOVE (@PRD) (0 Adj)(*1 SbPr + Sg + Nom Barrier Kui OR Koma OR CLBC)
              (*-1 SbPr + Sg + Nom BARRIER CLBC);
REMOVE (@PRD) (0 Adj)(*1 SbPr + Pl + Nom BARRIER Kui OR Koma OR CLBC)
              (*-1 SbPr + Pl + Nom BARRIER CLBC);
SELECT (A nom @PRD) (*-1 ("ole")BARRIER Subst + Nom OR CLB)
              (NEGATE *1 Subst + Nom OR (@SUBJ inf) BARRIER CLB);

#5) Substantiivse öeldistäite korral peab leiduma ka alus
#  voi peab verb olema esimeses 2 pöördes
LIST SbPrN = S P N ;
LIST Side = J;

REMOVE (S nom @PRD) OR (P nom @PRD) (NOT *-1 SbPrNom)
              (NOT *1 SbPrNom)(NOT *-1 Vps12);
REMOVE (S @PRD) OR (P @PRD) (NEGATE *0 SbPrNom OR SbPr + Par BARRIER CLB)
                            (NEGATE *-1 Vps12 BARRIER CLB);
REMOVE (S sg nom @PRD) OR (P sg nom @PRD) (1 Side)(2 SbPr + (sg nom))
              (NEGATE *3 SbPrNom BARRIER CLB)
              (NEGATE *-1 SbPrNom OR Vps12 BARRIER CLB);
REMOVE (S sg nom @PRD) OR (P sg nom @PRD) (-1 Side)(-2 SbPr +  (sg nom))
              (NEGATE *-3 SbPrNom OR Vps12 BARRIER CLB)
              (NEGATE *1 SbPrNom BARRIER CLB);
# Väljas oli ohtu voi öö.

# 6) Unikaalsus
# ei ole predikatiiv, kui osalauses juba leidub predikatiiv, mis ei ole
# antud sonaga koordinatsioonis "ja" voi "ning" abil
REMOVE (@PRD) (*1C (@PRD) BARRIER Sid|Kvm OR CLB)
              (NEGATE *1 ("ole" inf) BARRIER CLB);
REMOVE (@PRD) (*-1C (@PRD) BARRIER Sid|Kvm OR CLB);
# Vead: Must huumor see ainus õige ongi
# Praegune on juba lõpp ise
REMOVE (@PRD) (*1C (@ORD) BARRIER CLB LINK *-1 ("ole") BARRIER CLB)
              (NEGATE *-1 ("ole") BARRIER CLB);

#7) Osastavas nimisonaline öeldistäide
REMOVE (S part @PRD) OR (P part @PRD) (NOT -1 Adj OR (P dem))(NOT 0 ("miski" part));
REMOVE (part @PRD) (*0 (sg nom) BARRIER CLBC);
# see oli teatavat _liiki_ hüpnoos
REMOVE (sg part @PRD) (*-1 ("ole") BARRIER CLB 
                                LINK *1 (sg nom) BARRIER CLB LINK 1 Inf);
# nii kange oli mu tahtmine alustada iseseisvalt vanemate _otsinguid_
REMOVE (part @PRD) (*-1 ("ole") BARRIER CLB LINK *1 Adj + Nom BARRIER CLB);
REMOVE (pl part @PRD) ;



REMOVE (@PRD) (NOT 0 Inf)(*1 Inf ^ ("ole") BARRIER Kvm OR CLB)
              (NEGATE *1 (@SUBJ inf) BARRIER CLB);

#8) Koordinatsioon
SELECT (sg nom @PRD) (*-1 JaVoi BARRIER CLB LINK -1C (@PRD sg nom));
SELECT (pl nom @PRD) (*-1 JaVoi BARRIER CLB LINK -1C (@PRD pl nom));
SELECT (part @PRD) (*-1 JaVoi BARRIER CLB LINK -1C (@PRD part));

REMOVE (S nom @PRD) OR (P nom @PRD) 
              (*-1 JaVoiK BARRIER FinV OR CLB LINK -1 SbPrNom LINK NOT 0 (@PRD) );
REMOVE (S part @PRD) OR (P part @PRD) 
              (*-1 JaVoiK BARRIER FinV OR CLB LINK -1 SbPr + Par LINK NOT 0 (@PRD));
REMOVE (A sg nom @PRD) 
              (*-1 JaVoiK BARRIER FinV OR CLB LINK -1 (A sg nom) LINK NOT 0 (@PRD)); 
REMOVE (sg part @PRD) (1 Side)(2 (sg part))(NOT 2 (@PRD))(3 Kvm);
# voib-olla ei olnud kassi vaates mingit _kutset_ ega märguannet.

#9) Erandjuhud
# adjektiiv partitiivis ei ole predikatiiv, kui eelmine on "midagi"
REMOVE (@PRD) (0 Adj)(0 Par)(-1 ("miski" part));
# infinitiiv pole öeldistäide, kui osalauses ei leidu alust.
REMOVE (inf @PRD) (NEGATE *0 Subjekt BARRIER CLB);
SELECT ("väärt" @PRD) (NEGATE *1 SbPr BARRIER CLBC)(*-1 ("ole") BARRIER CLBC);
# ta on seda väärt, et ... 
REMOVE (A nom @PRD) (1 (A nom))(NOT 1 (@PRD));
