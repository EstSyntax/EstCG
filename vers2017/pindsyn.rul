                 "märka" "nenti" "näge" "oleta" "oota" "rääki" "taipa" "taju"
                 "tead" "tund" "tunnista" "väit" "usku";

REMOVE (S prop gen @ADVL) (NOT 1 Tud OR Ger OR Inf OR Tav)
               (NEGATE *-1 Võrdlus BARRIER CLB);
REMOVE (gen @ADVL)(NOT -1 Järgarv)(NOT 0 NimKv OR ("kord") OR Aeg OR Arvsõna)
              (NEGATE *1 NiNaTaGa BARRIER NPpiir OR CLB)
              (NOT 1 NudTudDes OR Inf OR Tav)(NEGATE *-1 Võrdlus BARRIER CLB);
REMOVE (gen @ADVL)(NOT -1 Järgarv)(NOT 0 NimKv OR ("kord") OR Aeg OR Arvsõna)
              (1 NiNaTaGa)(NEGATE *-1 Võrdlus BARRIER CLB);

              
              
# Mitmuses omastavas käändes nimisona pole määrus, kui ei järgne pikune-tüüpi omadussona
REMOVE (pl gen @ADVL)(NOT 1 Sid|Kvm)(NOT 1 NudTudDes)(NOT 1 Inf)(NOT 1 Tav);
REMOVE (pl gen @ADVL)(1 Sid|Kvm)(NEGATE *1 NiNaTaGa BARRIER CLB);

# Nimisonalised kvantorid ei ole määrused:
REMOVE (nom @ADVL)(0 NimKv)(NOT 0 Aeg)(NOT 1 Hiljem)
                (NOT -1 Iga)(NOT -1 Järgarv)
                (NEGATE *-1 Võrdlus BARRIER CLBC)(NEGATE *1 JärelKv BARRIER CLBC);

# Üksikud juhud:
REMOVE ("miski" part @ADVL);
REMOVE ("oma" @ADVL)(0 NGP)(NOT 1 Tav)(NOT 1 Tud)(NOT 1 Inf);


# Ma-tegevusnimi on määrus, kui osalauses leidub vastav verb
SELECT (sup @ADVL) (*-1C FV + SupIllAdvVerb BARRIER Sid|Kvm OR CLB);

# Lauselaiendid ja tervitused:
REMOVE (A nom @ADVL) (NOT 0 Lauselaiend);
SELECT (@ADVL) (0 Lauselaiend)(-1 <<<)(1 Koma);
SELECT (@ADVL) (0 Lauselaiend)(-2 <<<)(-1 ("väga"))(1 Koma);
SELECT (@ADVL) (0 Lauselaiend)(-1 <<<)(1 Küll);
SELECT (@ADVL) (0 Lauselaiend)(-1 Kvm)(1 Küll);
REMOVE (A nom @ADVL) (0 Lauselaiend)(NOT 1 Koma);


# nimisona seestütlevas on määrus,  kui järgneb keskvorre
SELECT (el @ADVL) (*1 Võrre BARRIER FV OR CLB);
SELECT (term @ADVL) (1 Täis);
# omadussona neljas viimases käändes on määrus
SELECT (@ADVL) (0 Adj + NiNaTaGa);
SELECT (A gen @ADVL) (1 JaVoi)(2 Adj + NiNaTaGa);
# omadussona omastavas pole määrus, kui paremal ei ole neljas viimases käändes omadussona.
REMOVE (A gen @ADVL) (NEGATE *1 Adj + NiNaTaGa);
# adverb enne partitsiipi on määrus
SELECT (D @ADVL) (1 Partic);
SELECT (digit @ADVL) (-1 OPar)(1 CPar);
REMOVE (digit @ADVL) (1 (Fst))(*1 Subst BARRIER CLB);
REMOVE (digit @ADVL) (1 (Fst))(2 ("a"&Y)); 
# Kui numbrina kirjutatud arvu järel on punkt ja see ei ole lause lopp.

SELECT (quot pres ps @ADVL) (NOT -1 Tud)(*0 VatAdvVerb BARRIER CLB);

AFTER-SECTIONS

# SUBJEKTI HEURISTILISED REEGLID 
# Need reeglid on kas nii uued, et on korralikult testimata, voi on nende tekitatud vigade hulk suur.
REMOVE (inf @SUBJ) (NEGATE *-1 TeisedDaInfv OR Emotsioonv OR Sobivusv 
                        OR Vajadusv OR Suutelisusv BARRIER CLBC)
		   (NEGATE *1 TeisedDaInfv OR Emotsioonv OR Sobivusv 
                        OR Vajadusv OR Suutelisusv BARRIER CLBC);

# Subjekt eelneb predikatiivile
SELECT (S @SUBJ) (NOT 0 (@OBJ) OR (@ADVL)) (1 FinV)
                (*2 SbPrNom + (@PRD) BARRIER CLBC);

# Nimetavas käändes sõnale järgneb kohe järgmine osalause
# Nt. Mees, kes ....
SELECT (nom @SUBJ) (NOT 0 Arv)(1 Koma OR OPar)(NEGATE *-1 Verb BARRIER CLB)
               (NEGATE *1 SbPr OR Verb BARRIER CLB)(NOT *-1 (Dsh)); # TP +Dsh

# Veidi laiendatud unikaalsuse printsiip, mis ei arvesta sidesõnade ja
# kvm-dega, sest leitud alus ja sõna on erinevates käänetes
REMOVE (@SUBJ) (NOT 0 Nom)(*-1C Subjekt + Nom)
                  (*-1 FinV BARRIER CLBC LINK NOT *-1 FinV);
REMOVE (@SUBJ) (NOT 0 Nom)(*1C Subjekt + Nom)
                  (*1 FinV BARRIER CLBC LINK NOT *1 FinV);
# Lause alguses on potentsiaalne subjekt, mis on nimetavas käändes,
# sõna ise on osastavas ja paremas kontekstis on infinitiiv.
 REMOVE (part @SUBJ) (*1 Inf BARRIER CLB)(*-1 Subjekt + Nom BARRIER CLB);
#Kui sõna on mitmuse nominatiivis ja järgneb vahetul verb pl3 ja
#rohkem mitmuse nominatiive osalauses pole
#vigu tekib poolikutes osalausetes. 
SELECT (pl nom @SUBJ) (1 (V ps3 pl)) (NOT *2 SbPr + Pl + Nom)
                  (NEGATE *0 Impl BARRIER CLB)
                  (NEGATE *-1 Subjekt BARRIER CLB);
# Tüüplause Ad Intr&YVVerb SUBJ
SELECT (@SUBJ) (NOT 0 Inf)
                  (*-1 In OR Ad BARRIER CLB 
                       LINK NEGATE *-1 SbPr BARRIER CLB)
                  (*-1 In OR Ad BARRIER Subjekt OR CLB LINK 1 Intr&YV)
                  (NEGATE *1 Subjekt BARRIER CLB)
                  (NEGATE *-1 Inf OR Tud BARRIER CLB)
                  (NEGATE *1 Inf OR Tud BARRIER CLB);
SELECT (@SUBJ) (NOT 0 Inf)(*-1 In OR Ad BARRIER CLB 
                               LINK NEGATE *-1 SbPr BARRIER CLB)
                  (*-1 In OR Ad BARRIER SbPrNom OR CLB LINK 1 Intr&YV)
                  (NEGATE *1 SbPrNom BARRIER CLB)
                  (NEGATE *-1 Tud BARRIER CLB)
                  (NEGATE *1  Tud BARRIER CLB)(1C (@<INFN));

SELECT (@SUBJ) (*-1 In OR Ad BARRIER CLB LINK NOT *-1 SbPr BARRIER CLB)
               (*-1 In OR Ad BARRIER CLB OR SbPrNom LINK 1 Intr&YV)
               (NEGATE *-1 Tud OR Imper BARRIER CLB)
               (NEGATE *1  Tud OR Imper BARRIER CLB)
               (*1C Objekt BARRIER CLB LINK *1 Inf BARRIER CLB);

SELECT (nom @SUBJ) (NOT 0 Arv)(NOT *0 Koma)
               (NOT *-1 Subjekt)(NOT *1 Subjekt)
               (NOT 1 Inf)(NOT *0 Impl)
               (NOT *0 Imper)(NOT *0 Vps12);

SELECT (nom @SUBJ) (NOT 0 Määrus)
               (NEGATE *-1 Koma BARRIER CLB)
               (*1 Koma BARRIER CLB LINK *1 SbPr BARRIER CLB)
               (NEGATE *0 Subjekt BARRIER CLB)
               (*1 CLB ^ Ja)(*-1 FinV BARRIER CLB)(NOT 1 Inf)
               (NEGATE *0 Impl BARRIER CLB)
               (NEGATE *0 Imper BARRIER CLB)
               (NEGATE *0 Vps12 BARRIER CLB)
               (NOT -1 Partic);
# välja jätta või maha tõmmata on lihtne
 SELECT (inf @SUBJ) (*1C (@PRD) + Adj)
          (NEGATE *0 SbPrNom BARRIER CLB);


# Koordinatsioon
SELECT (pl nom @SUBJ) (NOT 0 (P det))(-1 JaVoiK)(-2 Pl + Nom)(-2C Subjekt);
SELECT (pl nom @SUBJ) (1 JaVoiK)(2 Pl + Nom)(2C Subjekt);
SELECT (sg nom @SUBJ) (-1 JaVoiK)(-2 Sg + Nom)(-2C Subjekt)
               (NEGATE *1 Impl BARRIER CLBC);
SELECT (sg nom @SUBJ) (1 JaVoiK)(2 Sg + Nom)(2C Subjekt);
SELECT (nom @SUBJ) (*-1 JaVoi BARRIER Head OR CLBC LINK -1C Subjekt + Nom);
SELECT (nom @SUBJ) (*-1 Koma BARRIER Head LINK -1C Subjekt + Nom)
                      (*1 JaVoi BARRIER CLBC LINK *1 Subjekt BARRIER CLBC);

SELECT (@SUBJ) (0 KvAdv)(*-1 ("ole") BARRIER CLB)
               (NEGATE *0 Subjekt BARRIER CLB);

AFTER-SECTIONS

# osalause algab kes-või-mis, millele järgneb verbi sg 3 ja pole
# mineviku partitsiipi

LIST ParObjVerb = <Part> <Part-P>;
LIST Trans = <NGP-P>;

SELECT ("kes" @SUBJ) OR ("mis" @SUBJ) (-1 JaVoiK)(0 Sg)(1 (V sg ps3))
                   (NEGATE *1 Impl BARRIER CLB);
SELECT ("kes" @SUBJ) OR ("mis" @SUBJ) (-1 JaVoiK)(0 Pl)(1 (V pl ps3))
                   (NEGATE *1 Impl BARRIER CLB);


REMOVE (S @SUBJ) (*-1C (inf @SUBJ) BARRIER CLB);
REMOVE (@SUBJ)   (0 (@PRD))(NOT 0 Määrus)(NOT 0 Objekt)
                 (*-1 Prverb BARRIER (@PRD) OR NotEestäiend OR CLBC
                  LINK *-1C Subjekt BARRIER (@PRD) OR NotEestäiend OR CLBC);
SELECT (P pers pl ps3 nom @SUBJ) (*1 (V ps3 pl) BARRIER CLB) ;
REMOVE (S part @SUBJ) (*-1 FV + ParObjVerb BARRIER CLB)(NEGATE *1 Par BARRIER Kvm OR CLB);
REMOVE (S part @SUBJ) (*1 FV + ParObjVerb BARRIER CLB)(NEGATE *1 Par BARRIER Kvm OR CLB);
REMOVE (S part @SUBJ) (*-1 FV + Trans BARRIER CLB)(NEGATE *1 Par BARRIER Kvm OR CLB);
REMOVE (S part @SUBJ) (*1 FV + Trans BARRIER CLB)(NEGATE *1 Par BARRIER Kvm OR CLB);


# uued lisareeglid
# määruslik järeltäiend

LIST AllRekts = "suund" "mõte" ;
SELECT (S all @<NN) OR (P all @<NN) (*-1 AllRekts BARRIER CLB OR FinV);
LIST ElRekts = "otsus" "mõte" "ülevaade" "arvamus" ;
SELECT (S el @<NN) OR (P el @<NN) (*-1 ElRekts BARRIER CLB OR FinV);
LIST TrRekts = "nõus_olek" "valmidus" "valmis_olek" ;
SELECT (S tr @<NN) OR (P tr @<NN) (*-1 TrRekts BARRIER CLB OR FinV);
LIST InRekts = "läbi_viimine" "toimumine" ;
SELECT (S in @<NN) OR (P in @<NN) (*-1 InRekts BARRIER CLB OR FinV);
LIST KomRekts = "seos" ;
SELECT (S kom @<NN) OR (P kom @<NN) (*-1 KomRekts BARRIER CLB OR FinV);


SELECT (S all @ADVL) (*-1 ("vastavalt" D) BARRIER CLB OR FinV);

REMOVE (@<DN);
LIST HinnanguAdv = "küllalt" "liiga" "üsna" "väga" "tõesti" "täiesti" "peaaegu" "nii";
REMOVE (@DN>) (0 HinnanguAdv);
LIST LaiendiAdv = "aga" "seega" "juba" "veel" "ka" "samuti";
REMOVE (@DN>) (0 HinnanguAdv);

REMOVE (@<NN) (-1 (K pre));

#osaleda kodukoha arengu suunamises
REMOVE (@<NN) (NEGATE *-1 NotEestäiend BARRIER (V) OR CLB);
REMOVE (@NN>) (1C (@<Q))(NOT 2 Subst);

LIST TahabObjekti = "võt" "pane" ;

SELECT (@OBJ) (NEGATE *0 Objekt BARRIER CLB)(*0 TahabObjekti BARRIER CLB);
SELECT (@OBJ) (*-1 (inf @OBJ) BARRIER CLB LINK 0 TahabObjekti LINK NEGATE *-1 Objekt BARRIER CLB)(NEGATE *1 Objekt BARRIER CLB);
END
